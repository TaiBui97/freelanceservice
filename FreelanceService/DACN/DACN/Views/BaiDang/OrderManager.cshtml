                                    <tr>
                                        <th>Tên Người Bán</th>
                                        <th>Nội Dung</th>
                                        <th>Ngày Đặt</th>
                                        <th>Ngày Bắt Đầu</th>
                                        <th>Ngày Giao</th>
                                        <th>Giá</th>
                                        <th>Tình Trạng</th>
                                        <th></th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.myListDDH)
                                    {
                                        if (item != null)
                                        {
                                            var sp = Model.myBuyerListSP.Where(a => a.MaSP == item.MaSP).FirstOrDefault();
                                            var tk = Model.myBuyerListTK.Where(a => a.MaTaiKhoan == sp.MaTaiKhoan).FirstOrDefault();
