                                        <th>Tên Khách</th>
                                        <th>Nội Dung</th>
                                        <th>Ngày Đặt</th>
                                        <th>Ngày Bắt Đầu</th>
                                        <th>Ngày Giao</th>
                                        <th>Giá</th>
                                        <th>Tình Trạng</th>
                                        <th>Ngày Nhận Tiền</th>
                                        <th>Số Tiền Nhận</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.customerListDDH)
                                    {

                                        if (item != null && item.TrangThai == true)
                                        {
                                            var tk = Model.customerListTK.Where(a => a.MaTaiKhoan == item.MaTaiKhoan).FirstOrDefault();
                                            <tr>
                                                <td>@item.SanPham.TenSP</td>
                                                @if (tk != null)
                                                {
                                                    <td>@tk.TenTaiKhoan</td>
                                                }
                                                <td>@item.MoTaDDH</td>
                                                <td>@String.Format("{0:dd/MM/yyyy}", item.NgayDat)</td>
                                                <td>@String.Format("{0:dd/MM/yyyy}", item.NgayBatDau)</td>
                                                <td>@String.Format("{0:dd/MM/yyyy}", item.NgayGiao)</td>
                                                <td class="pricing-item">@item.GiaDDH đ</td>
                                                <td>Đã Giao</td>
                                                @if (item.NgayNhanTien == null)
                                                {
                                                    <td>Chưa Nhận</td>
                                                }
                                                else
                                                {
                                                    <td>
                                                        @String.Format("{0:dd/MM/yyyy}", item.NgayNhanTien)
